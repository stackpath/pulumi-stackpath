{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "compute/index.ts",
        "compute/network.ts",
        "compute/networkPolicy.ts",
        "compute/networkRoute.ts",
        "compute/networkSubnet.ts",
        "compute/workload.ts",
        "config/index.ts",
        "config/vars.ts",
        "index.ts",
        "provider.ts",
        "storage/bucket.ts",
        "storage/index.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
